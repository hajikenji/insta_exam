<p id="notice"><%= notice %></p>
<p>
  <strong>User name:</strong>
  <%= @user.user_name %>
</p>
<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>
<%  if  @user.id == current_user.id %>
  <%= link_to 'Edit', edit_user_path(@user) %> |
<% end %>
<%= link_to 'Back', users_path %>
<%= link_to '写真投稿へ', pictures_path %>
<div>
  <p>プロフィール写真</p>
  <% if @user.my_image.url && @user.my_image %>
    <%= image_tag @user.my_image.url %>
  <% else %>
    <%= image_tag "default.png" %>
  <% end %>
</div>
